{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@yunai/schema-form": ["src"],
      "@yunai/schema-form/*": ["src/*", "*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [".dumirc.ts", "src/**/*", "typings.d.ts"],
  "exclude": ["node_modules", "es", "dist"]
}
